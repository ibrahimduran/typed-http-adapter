      "path": "dist/typed-http-adapter.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^5.29.0",
    "**/@typescript-eslint/parser": "^5.29.0",
    "**/typescript": "^4.7.4",
    "**/ts-jest": "^28.0.5",
    "**/jest": "^28.1.1",
    "**/prettier": "^2.7.1",
    "**/eslint-config-react-app": "^6.0.0",
    "**/eslint": "^7.28.0"
